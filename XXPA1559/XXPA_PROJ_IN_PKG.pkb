            .pa_task_id
            FROM pa_tasks
            WHERE project_id = p_agr_rec.g_prj_tbl(c_prj_loop.l_prj_cnt)
            .prj_main_rec.pa_project_id
            and task_number = c_prj_task_loop.pa_task_number;
            EXCEPTION
            WHEN OTHERS THEN
            write_into_file('Task Number : ' ||
            c_prj_task_loop.pa_task_number);
