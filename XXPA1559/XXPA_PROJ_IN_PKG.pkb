                                              .pa_task_id || '**' || g_budget_tbl(budget_cnt).g_budget_line_in_tbl(t)
                                              .pm_task_reference || '**' || g_budget_tbl(budget_cnt).g_budget_line_in_tbl(t)
                                              .revenue);
                            END LOOP; --FOR t IN 1..g_tasks_in.COUNT LOOP

                        END IF; --l_baseline_funding_flag = 'N' THEN

                    EXCEPTION
					  WHEN OTHERS THEN
                        NULL;
                    END;
